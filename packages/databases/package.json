{
  "name": "databases",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "db:generate": "npx prisma generate",
    "db:migrate": "pnpm run db:prisma:migrate && pnpm run db:clickhouse:migrate",
    "db:prisma:migrate": "npx prisma migrate dev",
    "db:clickhouse:migrate": "npx dbmate --url clickhouse://default:@localhost:19000/default --migrations-dir clickhouse/migrations --no-dump-schema up",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force",
    "build": "tsc",
    "dev": "npx tsx watch src/index.ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "dbmate": "^2.6.0",
    "prisma": "^5.3.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.2",
    "@prisma/client": "^5.3.1",
    "@t3-oss/env-core": "^0.6.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "zod": "^3.22.2"
  }
}
